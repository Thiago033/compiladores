void main {
    let int num;
    let int result;

    num := readIO()

    result := factorial(num);

    printIO result;
}

def int factorial(int n) {
    let int fact;
    let int i;

    fact := 1;
    i := 1;

    while (i <= n) do {
        fact := (fact * i);
        i := (i + 1);
    };

    return fact;
}
